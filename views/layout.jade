		script.
			jQuery(document).ready(function($) {  
				$(window).load(function(){
					$('#preloader').fadeOut('slow',function(){$(this).remove();});
				});
			});
	body
		#preloader
		nav.nav-override
			img.brand-logo.center#logo(src='/images/Logo.png', style='height: 100%;')
			.nav-wrapper
				ul#nav-mobile.left.hide-on-med-and-down
					li
						a(href="/home#").nav-button.home-button Home
					li
						a(href="/home#").nav-button.about-button About
					li
						a(href="/home#").nav-button.portfolio-button Portfolio
		#z-fix
			block content